{
  "multipart": [
    {   "when": { "axis": "x", "up": "true" },
        "apply": { "model": "moreblocks:block/rope_up", "x": 90, "y": 90}
    },
    {   "when": { "axis": "x", "down": "true" },
        "apply": { "model": "moreblocks:block/rope_down", "x": 90, "y": 90}
    },
    {   "when": { "axis": "x", "knot": "true" },
        "apply": { "model": "moreblocks:block/rope_knot", "x": 90, "y": 90}
    },
    {   "when": { "axis": "y", "up": "true" },
        "apply": { "model": "moreblocks:block/rope_up"}
    },
    {   "when": { "axis": "y", "down": "true" },
        "apply": { "model": "moreblocks:block/rope_down"}
    },
    {   "when": { "axis": "y", "knot": "true" },
        "apply": { "model": "moreblocks:block/rope_knot"}
    },
    {   "when": { "axis": "z", "up": "true" },
        "apply": { "model": "moreblocks:block/rope_up", "x": 90}
    },
    {   "when": { "axis": "z", "down": "true" },
        "apply": { "model": "moreblocks:block/rope_down", "x": 90}
    },
    {   "when": { "axis": "z", "knot": "true" },
        "apply": { "model": "moreblocks:block/rope_knot", "x": 90}
    }
  ]
}